{
  "name": "@xor/mock-xor-ui",
  "version": "2.1.1",
  "description": "Dev host for running the app without any network dependencies",
  "main": "index.json",
  "author": "Jiri Kantor",
  "license": "MIT",
  "private": true,
  "scripts": {
    "lint": "eslint . --ext .tsx,.ts,.js --ignore-path ../../../.eslintignore",
    "clean": "rimraf lib",
    "build": "run-s lib:build client:build",
    "lib:build": "tsc",
    "client:build": "webpack --config webpack.client.config.js",
    "watch:server": "webpack --watch --config webpack.server.config.js",
    "watch:client": "webpack --watch --config webpack.client.config.js",
    "watch": "run-p watch:server watch:client"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.6.2",
    "@types/react-dom": "^17.0.11",
    "@xor/xor-adapters-mock": "^2.0.3",
    "@xor/xor-config": "^1.0.0",
    "@xor/xor-ui": "^2.3.0",
    "express": "^4.17.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "redux-thunk": "^2.4.0",
    "systemjs": "^6.11.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.6",
    "@types/webpack": "^4.41.31",
    "@types/webpack-env": "^1.16.3",
    "@xor/eslint-config": "^1.1.0",
    "@xor/eslint-config-react": "^1.1.0",
    "@xor/xor-domain": "^2.0.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^6.4.1",
    "css-loader": "^5.2.7",
    "html-webpack-plugin": "4.5.2",
    "less": "^4.1.2",
    "less-loader": "7.3.0",
    "mini-css-extract-plugin": "^1.6.2",
    "node-loader": "^1.0.3",
    "null-loader": "^4.0.1",
    "source-map-loader": "^1.1.3",
    "start-server-webpack-plugin": "^2.2.5",
    "ts-loader": "^8.3.0",
    "webpack": "^4.41.0",
    "webpack-cli": "^4.9.1"
  }
}
