{
  "name": "@xor/dev-xor",
  "version": "1.0.1",
  "description": "Dev host for running the app without any network dependencies",
  "main": "index.json",
  "author": "Jiri Kantor",
  "license": "MIT",
  "private": true,
  "scripts": {
    "lint": "eslint . --ext .tsx,.ts,.js --ignore-path ../../../.eslintignore",
    "clean": "rimraf lib",
    "watch:server": "webpack --watch --config webpack.server.config.js",
    "watch:client": "webpack --watch --config webpack.client.config.js",
    "watch": "run-p watch:server watch:client"
  },
  "dependencies": {
    "@types/react-dom": "^17.0.11",
    "@xor/ui": "^1.0.1",
    "express": "^4.17.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "systemjs": "^6.11.0",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@types/webpack": "^4.41.31",
    "@types/webpack-env": "^1.16.3",
    "@xor/eslint-config": "^1.0.0",
    "@xor/eslint-config-react": "^1.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^6.4.1",
    "html-webpack-plugin": "4.5.2",
    "node-loader": "^1.0.3",
    "null-loader": "^4.0.1",
    "source-map-loader": "^1.1.3",
    "start-server-webpack-plugin": "^2.2.5",
    "ts-loader": "^8.3.0",
    "typescript": "^4.4.4",
    "webpack": "^4.41.0",
    "webpack-cli": "^4.9.1"
  }
}
