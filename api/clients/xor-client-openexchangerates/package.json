{
  "name": "@xor/xor-client-openexchangerates",
  "version": "1.0.1",
  "description": "Client for communication with openexchangerates.org",
  "main": "./lib/index.js",
  "author": "Jiri Kantor",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rimraf ./lib",
    "lint": "eslint . --ext .ts,.js --ignore-path ../../../.eslintignore",
    "fetch": "env-cmd -f ./env.json run-s fetch:do",
    "fetch:do": "curl https://openexchangerates.org/api/latest.json -H \"Authorization: Token ${APP_ID}\""
  },
  "devDependencies": {
    "@xor/eslint-config": "^1.1.0",
    "@xor/typescript-config": "^1.0.0"
  }
}
